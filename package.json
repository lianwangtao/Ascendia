{
  "name": "ImageCat",
  "author": "Shane Lian",
  "version": "1.0.2",
  "scripts": {
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register --require ./test/test_helper.js",
    "lint": "eslint . --fix",
    "flow": "flow .",
    "webpack-watch": "webpack -w",
    "express-server": "nodemon ./server",
    "migrate": "sequelize db:migrate",
    "populate-cat": "babel-node ./scripts/populate_categories",
    "populate-images": "babel-node ./scripts/populate_images",
    "dev": "NODE_ENV=development concurrently --kill-others \"yarn run webpack-watch\" \"yarn run express-server\"",
    "prod": "NODE_ENV=production concurrently --kill-others \"NODE_ENV=production yarn run webpack-watch -p\" \"NODE_ENV=production yarn run express-server\"",
    "start": "NODE_ENV=production yarn run setupDb && yarn run populate-images && yarn run populate-cat && yarn run prod"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-regenerator": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "express": "^4.14.0",
    "fs": "^0.0.1-security",
    "helmet": "^3.4.0",
    "http-proxy": "^1.16.2",
    "https": "^1.0.0",
    "image-webpack-loader": "^3.3.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.16.4",
    "mocha": "^4.0.1",
    "moment": "^2.17.0",
    "pg": "6.4.1",
    "react": "^15.4.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^4.4.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sendinblue-api": "^1.0.8",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "sib-api-v3-sdk": "^3.1.4",
    "sqlite3": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "flow-bin": "^0.35.0",
    "jsdom": "^9.9.1",
    "nodemon": "^1.11.0",
